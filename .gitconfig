[core]
    editer = /usr/bin/vim
    filemode = false
    autocrlf = false
    symlinks = false
[alias]
    l = log --oneline
    s = status
    b = branch
    c = checkout
    d = diff
    dn = diff
    ds = diff --cached
    dd = !sh -c 'git diff ${1:-HEAD}~ ${1:-HEAD} $2 $3 $4 $5 $6 $7 $8 $9' -
    ibn = !sh -c 'git rebase -i HEAD~${1:-$(expr $(git rev-list HEAD | wc -l) - 1)}' -
    rs = rebase --skip
[color]
    ui = auto
